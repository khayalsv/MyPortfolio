@model EntityLayer.Concrete.Home

@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Member/Views/Shared/_MemberLayout.cshtml";
}

<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label>Title</label>
        <input type="text" asp-for="Title" class="form-control" />
    </div>

    <div class="form-group">
        <label>Text</label>
        <input type="text" asp-for="Text" class="form-control" />
    </div>

    <div class="form-group">
        <label>About</label>
        <input type="text" asp-for="About" class="form-control" />
    </div>

    <div class="form-group">
        <label>Name</label>
        <input type="text" asp-for="Name" class="form-control" />
    </div>

    <div class="form-group">
        <label>Facebook</label>
        <input type="text" asp-for="Facebook" class="form-control" />
    </div>

    <div class="form-group">
        <label>Github</label>
        <input type="text" asp-for="Github" class="form-control" />
    </div>

    <div class="form-group">
        <label>Linkedin</label>
        <input type="text" asp-for="Linkedin" class="form-control" />
    </div>

    <div class="form-group">
        <label>Instagram</label>
        <input type="text" asp-for="Instagram" class="form-control" />
    </div>

    <div class="form-group">
        <label>Image</label>
        <input type="file" asp-for="Image" class="form-control" />
    </div>
    <br />

    <button class="btn btn-info">Save</button>
</form>